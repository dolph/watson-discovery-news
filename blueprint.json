{
  "appIconURL": "blueprint/appicon.svg",
  "description": "Build a cognitive news search and trends web app using Node.js and the Watson Discovery service.",
  "email": "cloud@us.ibm.com",
  "generatorOptions": {
    "generator-core-python-flask": {
      "scope": "@arf"
    },
    "generator-ibm-cloud-enablement": {
      "version": "0.8.9"
    },
    "generator-ibm-core-node-express": {
      "version": "0.5.0"
    },
    "generator-ibm-java": {
      "java": {
        "options": {
          "createType": "skit/liberty"
        }
      },
      "options": {
        "buildType": "maven",
        "headless": "true"
      },
      "spring": {
        "options": {
          "createType": "skit/spring"
        }
      }
    },
    "generator-ibm-service-enablement": {
      "version": "0.8.1"
    },
    "generator-ibm-usecase-enablement": {
      "version": "3.2.0"
    },
    "generator-swiftserver": {}
  },
  "generators": {
    "node": [
      "generator-ibm-core-node-express",
      "generator-ibm-cloud-enablement",
      "generator-ibm-service-enablement",
      "generator-ibm-usecase-enablement"
    ]
  },
  "name": "Create a cognitive news search app",
  "previewURLs": [],
  "requiredCapabilities": [
    "discovery"
  ],
  "requiredCapabilitiesURLs": {
    "discovery": "https://console.bluemix.net/catalog/services/discovery"
  },
  "servicePlans": {
    "discovery": "lite"
  },
  "tags": [
    "cli",
    "node",
    "watson"
  ],
  "thumbnailURL": "blueprint/thumbnail.svg",
  "type": "WEB"
}